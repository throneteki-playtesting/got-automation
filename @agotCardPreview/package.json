    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": "^19.1.0"
  },
  "dependencies": {
    "classnames": "^2.5.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3"
  }
}
